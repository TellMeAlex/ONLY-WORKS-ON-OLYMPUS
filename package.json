{
  "name": "@TellMeAlex/only-works-on-olympus",
  "version": "0.2.1",
  "description": "A meta-orchestrator for oh-my-opencode that routes requests to specialized agents using intelligent matching rules",
  "license": "SUL-1.0",
  "module": "dist/index.js",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "opencode",
    "plugin",
    "meta-agent",
    "routing",
    "orchestrator",
    "ai-agents"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TellMeAlex/ONLY-WORKS-ON-OLYMPUS.git"
  },
  "homepage": "https://github.com/TellMeAlex/ONLY-WORKS-ON-OLYMPUS#readme",
  "bugs": {
    "url": "https://github.com/TellMeAlex/ONLY-WORKS-ON-OLYMPUS/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "bun run build:node && bun run build:bun && bun run schema:generate",
    "build:node": "bun build-node.mjs",
    "build:bun": "bun build src/index.ts --outdir dist-bun --target bun",
    "typecheck": "bunx tsc --noEmit",
    "schema:generate": "bun scripts/generate-schema.ts",
    "schema:validate": "bun scripts/validate-schema.ts",
    "olimpus": "bun bin/olimpus.ts",
    "test": "bun test",
    "publish:github": "npm publish"
  },
  "dependencies": {
    "oh-my-opencode": "^3.4.0",
    "@opencode-ai/plugin": "^1.1.19",
    "@opencode-ai/sdk": "^1.1.19",
    "zod": "^4.1.8",
    "jsonc-parser": "^3.3.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5",
    "esbuild": "^0.23.0"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "^1.1.19",
    "@opencode-ai/sdk": "^1.1.19"
  }
}
